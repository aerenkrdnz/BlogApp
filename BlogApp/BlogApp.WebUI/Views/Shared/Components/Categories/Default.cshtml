@model List<CategoryViewModel>

<div class="list-group">

	<a asp-controller="" asp-action="Projects" asp-route-categoryId="" asp-route-categoryName="" class="fw-bold list-group-item">Tüm Projeler</a>

	@foreach (var item in Model)
	{
		<a asp-controller="" asp-action="Projects" asp-route-categoryId="@item.Id" asp-route-categoryName="@item.Name" class="list-group-item">@item.Name</a>
	}


</div>